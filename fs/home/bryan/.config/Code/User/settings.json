{
    "editor.fontSize": 12,
    "editor.renderWhitespace": "all",
    "editor.cursorStyle": "block",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.rulers": [
        120,
        160
    ],
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.labelFormat": "short",
    "files.associations": {
        "Fastfile": "ruby",
        "Matchfile": "ruby",
        "Pluginfile": "ruby",
        "Appfile": "ruby",
        "*.schema": "json",
        "*.swiftinterface": "swift",
        "*.html.jinja": "jinja-html"
    },

    "[javascript]": {

        "editor.tabSize": 2,
    },
    "[ruby]": {
        "editor.tabSize": 2,
        "editor.semanticHighlighting.enabled": true,
        "editor.formatOnSave": false,
    },
    "[json]": {
        "editor.tabSize": 2,
    },
    "editor.autoClosingBrackets": "never",
    "editor.minimap.renderCharacters": false,
    "extensions.ignoreRecommendations": false,
    "editor.tabSize": 2,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.parameterHints.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.build": true,
        "**/.classpath": true,
        "**/.cloudsqlproxy": true,
        "**/.factorypath": true,
        "**/.mypy_cache": true,
        "**/.project": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.settings": true,
        "**/*.egg-info": true
    },
    "editor.suggestSelection": "recentlyUsed",
    "go.formatTool": "goimports",
    "ruby.format": false,
    "ruby.lint": {
        "rubocop": false
    },
    "eslint.alwaysShowStatus": true,
    "eslint.run": "onSave",
    "eslint.validate": [
        "javascript",
        "graphql",
    ],
    "[yaml]": {
        "editor.tabSize": 2
    },
    "editor.renderControlCharacters": true,
    "yaml.schemas": {
        "https://json.schemastore.org/github-action": ["action.yml"],
        "https://json.schemastore.org/github-workflow": ".github/workflows/*.yml",
    },
    "[shellscript]": {
        "editor.tabSize": 4,
        "files.eol": "\n"
    },
    "window.menuBarVisibility": "compact",
    "window.titleBarStyle": "custom",
    "screencastMode.fontSize": 24,
    "screencastMode.onlyKeyboardShortcuts": true,
    "workbench.editorAssociations": {
        "*.ipynb": "default"
    },
    "editor.accessibilitySupport": "off",
    "redhat.telemetry.enabled": false,
    "[dart]": {
        "editor.formatOnSave": false,
        "editor.formatOnType": false,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue",
        "markdown",
        "graphql"
    ],
    "git.confirmSync": false,
    "lldb.library": "/opt/apple/current/usr/lib/liblldb.so",
    "settingsSync.ignoredExtensions": [
        "ms-vscode.cpptools-extension-pack"
    ],
    "debug.onTaskErrors": "abort",
    "swift.path": "/opt/apple/current/usr/bin/",
    "settingsSync.keybindingsPerPlatform": false,
    "workbench.panel.defaultLocation": "left",
    "editor.minimap.enabled": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.excludeDirty": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 5,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "editor.wordBasedSuggestions": false,
    "window.newWindowDimensions": "inherit",
    "keyboard.dispatch": "keyCode",
    "terminal.integrated.defaultProfile.linux": "fish",
    "workbench.sideBar.location": "right",
    "workbench.panel.opensMaximized": "never",
    "window.enableMenuBarMnemonics": false,
    "window.dialogStyle": "custom",
    "zenMode.restore": false,
    "markdownlint.focusMode": true,
    "editor.autoClosingQuotes": "never",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.typeCheckingMode": "basic",
    "files.trimFinalNewlines": false,
    "files.insertFinalNewline": false,
    "editor.codeActionsOnSave": {

    },

    "editor.language.brackets": [

    ],
    "terminal.integrated.env.linux": {
        "TERM_INTEGRATED": "1"
    },
    "[python]": {
        "editor.tabSize": 4,
        "editor.formatOnType": false,
        "editor.defaultFormatter": "ms-python.python"
    },
    "editor.indentSize": "tabSize",
    "python.analysis.autoImportCompletions": true,
    "python.autoComplete.extraPaths": [

    ],
    "python.analysis.extraPaths": [],
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.hidesExplorerArrows": false,
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.folders.color": "#ef5350",
    "files.trimTrailingWhitespace": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.formatting.provider": "black",
    "python.testing.unittestEnabled": true,
    "python.linting.mypyEnabled": true,
    "python.diagnostics.sourceMapsEnabled": true,
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules/*/**",
        "**/.venv/*/**",
        "**/__pycache__",
        "**/.mypy_cache"
    ],
    "todo-tree.tree.tagsOnly": true,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.preview": true,
    "editor.suggest.showStatusBar": true,
    "editor.quickSuggestions": {
        "other": "inline"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope":[
                    "string.quoted.docstring.multi.python",
                    "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
                    "string.quoted.docstring.multi.python punctuation.definition.string.end.python",
                    "string.quoted.docstring.multi.python constant.character.escape.python"
                ],
                "settings": {
                    "foreground": "#01c459",
                    "fontStyle": "italic"
                }
            }
        ],
    },
    "workbench.colorCustomizations": {
        "[Default Light Modern]": {
            "tab.activeForeground": "#000000",
            "editor.findMatchBorder": "#000000",
        },
        "[Default Dark Modern]": {
            "editor.findMatchBorder": "#ffffff",
        }
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.stickyScroll.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "eslint.format.enable": true,
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "workbench.cloudChanges.autoResume": "off",
    "workbench.cloudChanges.continueOn": "off",
    "python.analysis.indexing": false,
    "python.analysis.fixAll": [],
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "python.analysis.gotoDefinitionInStringLiteral": true,
    "problems.defaultViewMode": "table",
    "python.linting.lintOnSave": true,
    "testing.defaultGutterClickAction": "debug",
    "testing.automaticallyOpenPeekViewDuringAutoRun": true,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.codeLens.enabled": false,
    "eslint.workingDirectories": [],
    "editor.codeLens": false,
    "json.schemas": [],
    "terminal.integrated.scrollback": 1000,
    "todo-tree.tree.autoRefresh": false
}